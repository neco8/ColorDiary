{
  "name": "color_diary_project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "postcss-normalize": "^9.0.0",
    "sass": "^1.32.6",
    "stylelint": "^13.9.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0"
  },
  "scripts": {
    "css/sass/stylelint": "./node_modules/.bin/stylelint ./color_diary/static/color_diary/scss/*.scss",
    "css/sass/compile": "./node_modules/.bin/sass ./color_diary/static/color_diary/scss/style.scss ./color_diary/static/color_diary/css/style.css --watch",
    "css/sass": "yarn run css/sass/stylelint && yarn run css/sass/compile",
    "css/postcss": "./node_modules/.bin/postcss ./color_diary/static/color_diary/css/style.css -r true --watch",
    "css": "yarn run css/sass && yarn run css/postcss"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {}
}
